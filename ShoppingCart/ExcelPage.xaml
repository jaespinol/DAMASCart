<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="ShoppingCart.ExcelPage"
             Title="Excel">


    <StackLayout Padding="12">
        <Label Text="Selecciona una opción" FontSize="Large"/>
        <Label Text="Productos" FontSize="Small"/>
        <Button x:Name="OpenFile" Clicked="OpenFile_Clicked" Text="Subir / Actualizar - productos"/>
        <Button x:Name="OpenStock" Clicked="OpenStock_Clicked" Text="Agregar Stock"/>
    </StackLayout>
    
</ContentPage>